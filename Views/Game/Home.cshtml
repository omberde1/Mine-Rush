@{
    ViewData["Title"] = "Home";
}

<!-- banner part start-->
<section class="banner_part">
    <div class="container">
        <div class="row align-items-center justify-content-between">
            <div class="col-lg-6 col-md-8">
                <div class="banner_text">
                    <div class="banner_text_iner">
                        <h1>Best Highlights
                            of the Latest</h1>
                        <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit,
                            sed do eiusmod tempor incididunt ut labore et dolore
                            magna aliqua. Quis ipsum </p>
                        <a href="#" class="btn_1">Watch Tutorial</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- banner part start-->

<!--::client logo part end::-->
<section class="client_logo">
    <div class="container">
        <div class="row align-items-center">
            <div class="col-lg-12">
                <div class="client_logo_slider owl-carousel d-flex justify-content-between">
                    <div class="single_client_logo">
                        <img src="~/Game_Template/img/client_logo/client_logo_1.png" alt="">
                    </div>
                    <div class="single_client_logo">
                        <img src="~/Game_Template/img/client_logo/client_logo_2.png" alt="">
                    </div>
                    <div class="single_client_logo">
                        <img src="~/Game_Template/img/client_logo/client_logo_3.png" alt="">
                    </div>
                    <div class="single_client_logo">
                        <img src="~/Game_Template/img/client_logo/client_logo_4.png" alt="">
                    </div>
                    <div class="single_client_logo">
                        <img src="~/Game_Template/img/client_logo/client_logo_5.png" alt="">
                    </div>
                    <div class="single_client_logo">
                        <img src="~/Game_Template/img/client_logo/client_logo_1.png" alt="">
                    </div>
                    <div class="single_client_logo">
                        <img src="~/Game_Template/img/client_logo/client_logo_2.png" alt="">
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!--::client logo part end::-->

<!-- use sasu part end-->
<section class="Latest_War">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-lg-8">
                <div class="section_tittle text-center">
                    <h2>Latest Live Stream</h2>
                </div>
            </div>
        </div>
        <div class="row justify-content-center align-items-center">
            <div class="col-lg-12">
                <div class="Latest_War_text">
                    <div class="row justify-content-center align-items-center h-100">
                        <div class="col-lg-6">
                            <div class="single_war_text text-center">
                                <img src="~/Game_Template/img/favicon.png" alt="">
                                <h4>Time Rush</h4>
                                <p>27 January , 2024</p>
                                <a href="#">View</a>
                                <div class="war_text_item d-flex justify-content-around align-items-center">
                                    <img src="~/Game_Template/img/war_logo_1.png" alt="">
                                    <h2>190<span>vs</span>189</h2>
                                    <img src="~/Game_Template/img/war_logo_2.png" alt="">
                                </div>

                            </div>
                        </div>
                    </div>
                    <a href="#" class="btn_2">Watch Live</a>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- use sasu part end-->

